{
  "name": "origin-tracer",
  "version": "0.2.1",
  "description": "Show a call graph from an etherium transaction trace",
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "author": "Daniel Von Fange",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "prettier":"prettier --write src/*.ts",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "check-types": "tsc"
  },
  "babel": {
    "presets": [
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "babel-loader": "^8.0.4",
    "prettier": "1.18.2",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
}
